
<body  style="background-color:#E1DBDB">
	<h1 style="text-align:center;font-size:50px;color:rgb(66,69,86)">
    Search Result
</h1>

<% for(var i=0;i<data["Search"].length;i++){ %>
<div style="margin:20px auto;border-radius:15px;border:2px solid rgb(66,69,86);padding:30px;max-width:50%">
	<div  style="text-align:center;margin-bottom:10px">
		<img style="height:150px;width:150px" src=<%=data["Search"][i].Poster%>>
    </div>
    <li style="text-align:center;list-style:none;font-size:20px;color:rgb(66,69,86)" ><strong><%= data["Search"][i].Title	 %>       </strong> - <%= data["Search"][i].Year	 %></li>
</div>

	
<% } %>	
</body>
			